<%= form_with method: :get do |form| %>
  <%= form.label :filter_file, "Filter filename:" %>
  <%= form.text_field :filter_file, :value => params[:filter_file] %>&nbsp;
  <%= form.label :filter_struct, "Filter struct/member:" %>
  <%= form.text_field :filter_struct, :name => 'filter', :value => params[:filter] %>&nbsp;
  <%= yield form %>
  <%= form.submit 'Submit' %>
<% end %>
